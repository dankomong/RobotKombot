<% @user.errors.full_messages.each do |error| %>
 <%= error %>
 <% end %>



<%= form_tag "/login" do %>
  <ul>
    <li>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </li>
    <li>
      <%= label_tag :password %>
      <%= password_field_tag :password_digest %>
    </li>
  </ul>
  <%= submit_tag "log in" %>
<% end %>
